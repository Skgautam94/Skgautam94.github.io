@font-face {
  font-family: 'Raleway';
  src: url('../fonts/Raleway-Regular.ttf');
}

@font-face {
  font-family: 'Raleway-semibold';
  src: url('../fonts/Raleway-SemiBold.ttf');
}

@font-face {
  font-family: 'Raleway-bold';
  src: url('../fonts/Raleway-Bold.ttf');
}

@keyframes fadeIn {
  100% { opacity: 1 };
}

@keyframes flipInX {
  from {
    -webkit-transform: perspective(400px) rotateY(90deg);
    transform: perspective(400px) rotateY(90deg);
    -webkit-animation-timing-function: ease-in;
    animation-timing-function: ease-in;
    opacity: 0;
  }

  40% {
    -webkit-transform: perspective(400px) rotateY(-20deg);
    transform: perspective(400px) rotateY(-20deg);
    -webkit-animation-timing-function: ease-in;
    animation-timing-function: ease-in;
  }

  60% {
    -webkit-transform: perspective(400px) rotateY(10deg);
    transform: perspective(400px) rotateY(10deg);
    opacity: 1;
  }

  80% {
    -webkit-transform: perspective(400px) rotateY(-5deg);
    transform: perspective(400px) rotateY(5deg);
  }

  to {
    -webkit-transform: perspective(400px);
    transform: perspective(400px);
    opacity: 1;
  }
}

@keyframes popDown {
  0% { transform: translateY(-100px) }
}

@keyframes slideUp {
  0% { 
    transform: translateY(300px) 
  }
  100% { 
    opacity: 1;
    transform: translateY(0);
  }
}

@keyframes slideDown {
  0% { 
    transform: translateY(-300px) 
  }
  100% { 
    opacity: 1;
    transform: translateY(0);
  }
}


@keyframes slideInLeft {
  0% { 
    opacity: 0;
    transform: translateX(-300px) 
  }
  100% {
    opacity: 1;
    transform: translateX(0) 
  }
}

@keyframes slideInRight {
  0% { 
    opacity: 0;
    transform: translateX(300px) 
  }
  100% {
    opacity: 1;
    transform: translateX(0) 
  }
}

@keyframes toBottomFromTop {
  49% {
    transform: translateY(100%);
  }
  50% {
    opacity: 0;
    transform: translateY(-100%);
  }
  51% {
    opacity: 1;
  }
}

@keyframes popIn {
  0% {
    transform: scale(0);
  }
  60% {
    opacity: 1;
    transform: scale(1.05);
  }
  80% {
    transform: scale(0.95);
  }
  100% {
    transform: scale(1);
    opacity: 1;
  }
}

a {
  text-decoration: none;
  color: inherit;
}


body {
  color: #fff;
  font-family: 'Raleway';
  text-align: center;
  margin: 0px;
  font-size: 8pt;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  overflow-x: hidden;
}

.disable-hover {
  pointer-events: none;
}

.overflow-wrap {
  overflow-x: hidden;
}

.canvas {
  position: absolute;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100%;
  z-index: -1;
  background: url('../img/filler.png') center center/cover;
}

.button {
  padding: 10px 20px;
  border: 2px solid #fff;
  box-sizing: inherit;
  display: inline-block;
  font-size: 13pt;
  transition: all .5s;
  cursor: pointer;
  &:hover {
    background-color: $primary;
    border-color: $primary;
  }

  .mdi {
    vertical-align: middle;
  }
}

.bold {
  font-family: 'Raleway-bold';
}

.flex {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;

  &.row {
    flex-direction: row;
  }

  &.wrap {
    flex-wrap: wrap;
  }
}

.hex-wrap {
  text-align: center;
  position: relative;
  display: inline-block;
  width: 80px;
  height: 80px;
}

.hexagon {
  background-color: $primary;
  height: 100%;
  width: calc(100% * 0.57735);
  display: inline-block;
}

.hexagon:before {
  position: absolute;
  top: 0;
  right: calc((100% / 2) - ((100% * 0.57735) / 2));
  background-color: inherit;
  height: inherit;
  width: inherit;
  content: '';
  transform: rotateZ(60deg);
}

.hexagon:after {
  position: absolute;
  top: 0;
  right: calc((100% / 2) - ((100% * 0.57735) / 2));
  background-color: inherit;
  height: inherit;
  width: inherit;
  content: '';
  transform: rotateZ(-60deg);
}

.highlight {
  color: $secondary;
}

.header {
  font-size: 25pt;
  font-family: 'raleway-bold';
  position: relative;
  color: #444649;
  position: relative;
}

.header-bar {
  margin: 25px 0 75px 0;
  width: 70px;
  height: 4px;
  background: #444649;
}

.label {
  font-size: 14pt;
  font-weight: 500;
}

// NAV
nav {
  @include pos($left: 0, $bottom: 0);
  height: 50px;
  width: 100%;
  background: $primary-darkest;
  border-bottom: 3px solid $primary;
  text-align: left;
  z-index: 99;

  &.fixed {
    @include pos($pos: fixed, $top: 0, $bottom: inherit, $left: 0);
    &.desk {
      animation: popDown .5s;
    }
  }

  .link-wrap {
    max-width: 1200px;
    width: 100%;
    position: absolute;
    top: 53px;
    background: #333;
    overflow: hidden;
    height: 0;
    transition: height .3s ease-out;

    &.visible {
      height: 220px;
    }

    div {
      &:first-child {
        padding: 25px 20px 12px 20px;
      }
      padding: 12px 20px;
      font-size: 12pt;
      cursor: pointer;
      transition: color .5s;
      text-transform: uppercase;
      &:hover {
        color: $secondary;
      }
    }
  }

  .mdi-menu {
    position: absolute;
    right: 5vw;
    margin: auto 0;
    font-size: 30pt;
    line-height: 36pt;
    top: 0;
  }

  .active {
    color: $secondary;
  }
}

.carousel-wrap {
  position: relative;
  height: 250px;
  margin: 0 auto;
}

.window {
  overflow: hidden;
  position: relative;
  background: #222;
}

#carousel {
  width: 10000px;
  position: relative;
  top: 0;
}

.slide {
  height: 250px;
  cursor: pointer;
  float: left;
  display: flex;
  flex-direction: column;
  justify-content: center;
}


.mdi-chevron-left, .mdi-chevron-right {
  cursor: pointer;
  position: absolute;
  bottom: 0;
  background: rgba(0, 0, 0, 0.3);
  padding: 10px 15px;
  cursor: pointer;
  font-size: 14pt !important;
  font-weight: bold;
  color: #fff;
}

.mdi-chevron-left { left: 0 }
.mdi-chevron-right { right: 0 }

.transition {
  transition: .7s;
}

// MODAL
.modal-wrap {
  height: 100vh; width: 100vw;
  position: fixed;
  pointer-events: none;
  z-index: 100;

  .mask {
    height: 100%; width: 100%;
    position: absolute;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.5);
    opacity: 0;
    transition: opacity .3s;
  }

  .modal {
    background: #fff;
    box-shadow: 0 0 10px rgba(0, 0, 0, 0.4);
    position: relative;
    transition: all .3s;
    transform: scale(0.8);
    opacity: 0;
    align-items: flex-start;
    text-align: left;
    box-sizing: border-box;
    color: #333;
    height: 100vh;
  }

  .close {
    bottom: 25px;
    right: 10px;
    color: #bbb;
    font-size: 20pt;
    position: absolute;
    font-weight: 800;
    cursor: pointer;
  }

  .info-box {
    border-top: 3px solid $primary-darkest;
    padding: 15px 10px 85px 10px;
    box-sizing: border-box;
    color: #444;
  }

  .title {
    font-size: 16pt;
    line-height: 20pt;
    font-family: 'Raleway-bold';
  }

  .tag {
    display: block;
    font-weight: 300;
    color: #c0c0c0;
    font-family: 'Raleway-bold';
    border-bottom: 1px solid rgba(0,0,0,0.1);
    padding-bottom: 15px;
    font-size: 8pt;
  }

  .detail {
    font-size: 9pt;
    line-height: 14pt;
    margin-top: 13px;
  }

  .mdi-open-in-new {
    position: relative;
    left: -10px;
    font-size: 11pt;
  }

  .button {
    padding: 11px 30px;
    font-size: 10pt;
    transition: all .5s;
    cursor: pointer;
    background: $secondary;
    color: #fff;
    font-weight: 600;
    border: 0;
    pointer-events: none;
    visibility: hidden;
    position: absolute;
    bottom: 20px;
    left: 10px;

    &.visible {
      pointer-events: initial;
      visibility: visible;
    }

    &:hover {
      color: $secondary;
      background: #fff;
    }
  }

  .carousel {
    box-shadow: 10px 10px 5px 0px rgba(0,0,0,0.25);
  }

  &.visible {
    pointer-events: initial;
    .modal {
      transform: scale(1);
      opacity: 1;
    }
    .mask {
      opacity: 1;
    }
  }
}

section {
  padding: 70px 0 70px 0;
  color: #616161;
  line-height: 11pt;
  position: relative;

  .container {
    max-width: 1200px;
    margin: 0 auto;
    padding: 0 10px;
    width: 100%;
    box-sizing: border-box;
  }

  //HOME 
  &:nth-of-type(1) {
    padding: 0 10px;
    color: #fff;
    height: calc(100vh + 53px);

    .highlight {
      font-family: 'Raleway-semibold';
    }

    .text {
      margin: 0 0 20px 0;
      font-size: 16pt;
      line-height: 20pt;
    }

    .hexagon {
      transition: transform .9s ease-in-out;
      &:hover {
        transform: rotate(540deg);
      }
    }

    .button {
      padding: 12px 40px 12px 15px;
      position: relative;
      margin-bottom: 53px;
      
      .mdi {
        transition: transform .3s;
        position: absolute;
        right: 15px;
        top: 11px;
      }

      &:hover {
        .mdi {
          transform: rotate(90deg)
        }
      }

    }
  }

  //ABOUT
  &:nth-of-type(2) {
    
    .label-wrap {

      .label {
        margin: 20px 0 10px 0;
      }
    }

    .bullet-wrap {
      justify-content: flex-start;
      padding: 0 7px;
      height: 200px;
    }

    .highlight {
      color: #009ADA;
    }

    .up-arrow {
      margin: 15px 0;
      height: 30px;
    }

    .skills-wrapper {
      align-items: flex-start;
      width: 100%;

      .me {
        height: 200px;
      }

      .label {
        margin: 20px 0 15px 0;
      }
    }

    .hexagon i {
      position: absolute;
      top: 20px;
      font-size: 34pt;
      color: #fff;
      left: 0;
      right: 0;
      z-index: 1;
    }

    .bars-wrap {
      margin: 50px auto 0 auto;
      width: calc(100% - 15px);
      max-width: 90%;

      &:not(.animated) {
        .fill {
          width: 0 !important;
        }
      }
    }

    .bar {
      color: #666;
      height: 22px;
      position: relative;
      background-color: #eee;
      margin: 0 0 12px 0;
      align-items: flex-end;
      font-size: 7pt;

      @for $i from 1 through 10 {
        &:nth-child(#{$i}) .fill {
          transition: #{1 + ($i * .1)}s .2s width ease-in-out;
        }
      }

      span {
        display: none;
      }

      &.fill {
        @include pos($top: 0, $left: 0);
        background-color: $primary-dark;
        padding: 0;
        opacity: 1;
      }

      .tag {
        @include pos($top: 0, $left: 0);
        background-color: $primary;
        color: #fff;
        height: 100%;
        width: 110px;
        width: 70px;
      }

      &.secondary {
        background-color: $secondary-dark;
        .tag {
          background-color: $secondary;
        }
      }

      span {
        z-index: 1;
        padding: 0 15px 0 0;
      }
    }
  }

  //PORTFOLIO
  &:nth-of-type(3) {
    background: #f5f5f5;

    .container {
      padding: 0;
    }

    .filter-wrap {
      position: relative;
    }

    .filter {
      font-size: 6pt;
      font-weight: 600;
      padding: 0 20px;
      cursor: pointer;
      transition: background-color .5s;
    }

    .float-bar {
      @include pos($top: 0, $left: 30px, $bottom: 0);
      margin: auto 0;
      position: absolute;
      overflow: hidden;
      height: 25px; 
      border-radius: 1px;
      background: $secondary;
      transition: all .5s ease-in-out;


      .row {
        @include pos($top: 0, $left: -30px, $bottom: 0);
        margin: auto 0;
        transition: left .5s ease-in-out;
        .filter {
          color: #fff;
        }
      }
    }

    .text { 
      @include pos($top: 0, $left: 0);
      width: 100%;
    }

    #gallery {
      margin: 20px 0;

      .text, .button {
        z-index: 2;
        text-align: center;
        opacity: 0;
        transition: all 0.3s ease-in-out;
        color: $primary-darkest;

        .bold {
          font-size: 16pt;
        }
      }

      .button { 
        @include pos($bottom: 0, $left: 0, $right: 0);
        border: 2px solid $secondary;
        width: 170px;
        margin: 0 auto;
        font-size: 13pt;
        padding: 7px 0;
      }

      .mix {
        display: none;
        position: relative;

        .waypoint {
          background: #fff;
        }

        .button {
          &:hover {
            background: $secondary;
            color: #fff;
          }
        }
        
        @for $i from 1 through 12 {
          &:nth-child(#{$i}) {
            .card {
              background: url('../img/thumb-#{$i}.jpg') center center/cover;
            }
          }
        }

        .card {
          height: 240px;
          max-width: 100%;
          width: calc(50vw - 8px);
          cursor: pointer;
          transition: opacity .5s;
          
        }

        &:hover {
          .card { opacity: 0 }

          .text {
            opacity: 1;
            top: 24%;
          }

          .button {
            opacity: 1;
            bottom: 24%;
          }
        }


      }


    }
  }
  
  .flex-50-gt-sm { display: block;}


  //BLOG
  &:nth-of-type(4) {
    padding: 130px 0 100px 0;
    text-align: left;

    .flex.row-gt-sm, .flex.wrap {
      align-items: flex-start;
    }

    .gradient {
      position: absolute;
      top: 0;
      left: 0;
      height: 100%;
      width: 100%;
    }

    .blog-wrap {
      margin: 0 0 50px 0;
      padding: 0 2px;
      width: 100%;
      cursor: pointer;

      @for $i from 1 through 4 {
        #thumb-#{$i} {
          background: url('../img/blog-#{$i}.jpg') center center/cover;
        }
      }

      .fade-wrap {
        transition: opacity .3s;
      }

      &.fade .fade-wrap {
        opacity: 0.4;
      }

      &.hover .fade-wrap {
        .label {
          color: $primary;
        }
      }
    }

    .label {
      line-height: 14pt;
      font-size: 12pt;
      transition: color .3s ease-in-out;
    }

    .thumb {
      height: 150px;
      margin: 0 0 15px 0;
    }

    .date {
      font-size: 7pt;
      font-family: 'Open sans';
      margin: 5px 0 0 0;
    }

    .bar {
      height: 3px;
      width: 25%;
      background: $primary;
      margin: 10px 0px;
    }

    .text {
      font-size: 9pt;
      line-height: 13pt;
      max-height: 85px;
    }
  }

  //CONTACT
  &:nth-of-type(5) {
    background: #252934;
    color: #fff;
    padding-top: 175px;

    .header {
      color: #fff;
    }
    .header-bar {
      margin: 25px 0 40px 0;
      background: #fff;
      width: 110px;
    }
    .highlight {
      font-weight: 600;
      color: $primary;
    }

    svg {
      position: absolute;
      top: 0;
      left: 0;
    }

    form {
      margin: 40px auto 0 auto;
      min-width: 95%;
    }

    input[type="text"], input[type="email"], textarea {
      display: block;
      width: 100%;
      box-sizing: border-box;
      outline: none;
      border: 0;
      padding: 10px 15px;
      background: #1E242C;
      font-size: 12pt;
      margin-bottom: 3px;
      color: #fff;
    }

    textarea {
      min-height: 150px;
      margin-bottom: 5px;
    }

    .button {
      float: right;
      margin: 5px 0 0 0;
      font-size: 12pt;
      padding: 10px 30px;
      color: #fff;
      background: transparent;
      outline: 0;

      &:hover {
        background: $primary;
      }
    }

    #success {
      height: 0;
      width: 100%;
      overflow: hidden;
      position: relative;
      transition: height .25s;

      &.expand {
        height: 36px;
      }

      div {
        background: #04c986;
        color: #157051;
        font-size: 10pt;
        font-weight: 600;
        padding: 5px 0;
        border: 1px solid #157c59;
      }
    }

    #close {
      position: absolute;
      padding: 5px 15px;
      font-size: 12pt;
      font-weight: 600;
      right: 0;
      cursor: pointer;
    }
  }
}

footer {
  background: $primary-darkest;
  padding: 70px 0 50px 0;
  position: relative;

  .mdi-chevron-double-up {
    position: absolute;
    top: -25px;
    background: $secondary;
    font-size: 30pt;
    width: 47px;
    height: 50px;
    line-height: 40pt;
    left: 0;
    right: 0;
    margin: 0 auto;
    cursor: pointer;
    transition: background .5s;
    &:hover {
      background: $secondary-light;
    }
  }

  .icon-wrap {
    .icon {
      overflow: hidden;
      transition: background 0.3s, color 0.3s, box-shadow 0.3s, transform 0.3s;
      cursor: pointer;
      width: 55px;
      height: 55px;
      position: relative;
      text-align: center;
      margin: 0 15px;
      background: #262F38;
      font-size: 18pt;

    }

    .mdi {
      text-align: center;
      color: #fff;

      &.mdi-codepen {
        font-size: 20pt;
      }
    }

    .icon:hover {
      background: $primary;
      box-shadow: 0 0 0 3px rgba(4,94,201,0.1);
      transform: scale(0.9);

      .mdi {
        color: #fff;
      }
    }

    .icon:hover .mdi {
      -webkit-animation: toBottomFromTop 0.3s forwards;
      -moz-animation: toBottomFromTop 0.3s forwards;
      animation: toBottomFromTop 0.3s forwards;
    }
  }

  .footnote {
    font-size: 10pt;
    color: #8F9AA7;
    margin-top: 35px;
    opacity: 0.6;
    
    .highlight {
      font-family: 'Open Sans';
      color: $secondary;
    }
  }
}

// BREAKS
@media screen and (max-width: 400px) {
  section:nth-of-type(3) {
    #gallery .mix .card {
      width: 100vw;
    }
  }
}

@media screen and (min-width: 600px) {

  body {
    font-size: 12pt;
  }

  .container {
    padding: 0 20px;
  }

  .header {
    font-size: 30pt;
  }

  .header-bar {
    margin: 25px 0 100px 0;
  }

  .label {
    font-size: 18pt;
  }

  .hex-wrap {
    height: 100px; width: 100px
  }

  .slide, .carousel-wrap {
    height: 450px;
  }

  .mdi-chevron-left, .mdi-chevron-right {
    font-size: 25pt !important;
  }

  .modal-wrap {
    .modal {
      top: -6vh;
      height: initial;
    }
    .info-box {
      padding: 35px 60px 90px 25px;
    }

    .title {
      font-size: 22pt;
      line-height: 30pt;
    }

    .tag {
      font-size: 11pt;
    }

    .detail {
      font-size: 11pt;
      line-height: 18pt;
      margin-top: 15px;
    }

    .close {
      right: 25px;
    }

    .button {
      left: 25px;
    }
  }

  nav {
    .link-wrap {
      position: initial;
      background: none;
      overflow: visible;
      height: initial;
      text-align: left;

      .visible {
        height: initial;
      }

      div {
        &:first-child {
          padding: 0;
        }
        margin: 0 20px;
        padding: initial;
        font-size: initial;
        display: inline;
      }
    }

    .mdi-menu {
      display: none;
    }
  }

  section {
    line-height: 18pt;
    padding: 100px 0 130px 0;

    .container {
      padding: 0 10px;
    }

    // HOME 
    &:nth-of-type(1) {
      .text {
        font-size: 32pt;
        line-height: 36pt;
      }

      .button {
        padding: 12px 50px 12px 20px;
        
        .mdi {
          top: 16px;
        }
      }
    }

    // ABOUT
    &:nth-of-type(2) {
      .label-wrap {
        .label {
          margin: 15px 0 5px 0;
        }
        > div {
          padding: 0 15px;
        }
      }

      .bullet-wrap {
        height: 230px;
      }

      .skills-wrapper {

        > .flex:first-child {
          padding: 0 50px 0 50px;
        }

        .me {
          height: 250px;
        }
      }


      .bar {
        height: 28px;
        margin: 0 0 15px 0;
        font-size: 11pt;

        span {
          display: initial;
        }

        .tag {
          width: 110px;
        }
      }


      .hexagon i {
        font-size: 40pt;
      }
    }

    // PORTFOLIO
    &:nth-of-type(3) {

      .filter {
        font-size: 11pt;
        margin: 0 15px;
        padding: 0 30px;
      }

      .float-bar {
        height: 30px; 
      }

      #gallery {
        .mix {
          .card {
            height: 300px;
          }
        }
      }
    }


    &:nth-of-type(4) {
      padding: 130px 0 180px 0;
      .label {
        line-height: 22pt;
        font-size: 16pt;
      }

      .date {
        font-size: 8.5pt;
      }

      .bar {
        margin: 15px 0px;
      }

      .text {
        font-size: 11pt;
        line-height: 15pt;
      }

      .blog-wrap {
        width: 400px
      }

      .thumb {
        height: 200px;
      }
    }

    &:nth-of-type(5) {
      form {
        min-width: 500px;
      }
    }
  }

}

@media screen and (min-width: 960px) {

  .waypoint {
    opacity: 0;
  }

  .animated.waypoint#gallery {
    opacity: 1;
    .mix {
      @for $i from 1 through 6 {
        &:nth-child(#{$i}) > div {
          opacity: 0;
          animation: slideUp .75s #{$i * 0.1}s ease both;
        }
      }
    }
  }

  .animated {
    &.slide-up {
      animation: slideUp .75s ease both;
    }
    &.slide-down {
      animation: slideDown .75s ease both;
    }
    &.slide-in-left {
      animation: slideInLeft .75s ease both;
    }
    &.slide-in-right {
      animation: slideInRight .75s ease both;
    }
    &.flip-in-x {
      animation: flipInX .75s ease both;
    }
    &.fade-in {
      animation: fadeIn .75s ease both;
    }
    &.pop-in {
      animation: popIn 1s both;
    }
  }

  .canvas {
    background: #252934;
  }

  .button {
    font-size: 16pt;
  }

  .flex.row-gt-sm {
    flex-direction: row;
  }

  .flex-50-gt-sm {
    -webkit-flex: 1 1 50%;
    -ms-flex: 1 1 50%;
    flex: 1 1 50%;
    max-width: 50%;
    max-height: 100%;
    box-sizing: border-box;
  }



  section {
    // ABOUT
    &:nth-of-type(2) {
      .bars-wrap {
        margin: 0 0 0 0;
      }

      .skills-wrapper {
        margin: 50px 0 0 0;

        .me {
          margin: 0;
        }
      }

      .bar {
        max-width: 90%;
      }

      .bullet-wrap {
        margin: 0;
        padding: 0 10px;
        height: intial;
      }
    } 

    &:nth-of-type(3) {
      #gallery .mix .card {
        width: 390px;
      }
      .filter {
        font-size: 13pt;
        padding: 0 40px;
        margin: 0 25px;
      }
    }

    &:nth-of-type(4) {
      .blog-wrap {
        width: 400px;
        margin: 0 0 50px 20px;
        opacity: 0;
      }
    }
  }
}

@media screen and (min-width: 1280px) {
  section {
    &:nth-of-type(4) {
      .blog-wrap {
        width: 270px;
        margin: 0 0 50px 20px;
      }
    }
  }

}
